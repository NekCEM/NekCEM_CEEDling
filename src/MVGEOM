C
C     Moving mesh data
C
      COMMON /WSOL/   WX    (1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
     $            ,   WY    (1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
     $            ,   WZ    (1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
      COMMON /WLAG/   WXLAG (1,1,1,1,1) !(LX1M,LY1M,LZ1M,LELT,LORDER-1)
     $            ,   WYLAG (1,1,1,1,1) !(LX1M,LY1M,LZ1M,LELT,LORDER-1)
     $            ,   WZLAG (1,1,1,1,1) !(LX1M,LY1M,LZ1M,LELT,LORDER-1)
      COMMON /WMSU/   W1MASK(1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
     $            ,   W2MASK(1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
     $            ,   W3MASK(1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
     $            ,   WMULT (1,1,1,1) !(LX1M,LY1M,LZ1M,LELT)
      COMMON /EIGVEC/ EV1   (1,1,1,1) !(LX1M,LY1M,LZ1M,LELV)
     $              , EV2   (1,1,1,1) !(LX1M,LY1M,LZ1M,LELV)
     $              , EV3   (1,1,1,1) !(LX1M,LY1M,LZ1M,LELV)
      real  wx,wy,wz,wxlag,wylag,wzlag
      real  w1mask,w2mask,w3mask,wmult,ev1,ev2,ev3
