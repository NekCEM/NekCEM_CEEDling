      COMMON /NMNPECRBCV/
     $                ncemface_nmn(ldimt)
     $             ,  ncemface_pec(ldimt)
     $             ,  ncemface_r  (ldimt)
     $             ,  cemface_nmn(lxzfl,ldimt)
     $             ,  cemface_pec(lxzfl,ldimt)
     $             ,  cemface_r  (lxzfl,ldimt)
      COMMON /NMNPECRBCR/
     $                rhs_nmn  (lxzfl)
     $             ,  rhs_pec  (lxzfl)
     $             ,  rhs_r    (lxzfl)
      integer         ncemface_nmn,cemface_nmn
      integer         ncemface_pec,cemface_pec
      integer         ncemface_r,cemface_r
      real            rhs_nmn,rhs_pec,rhs_r  
